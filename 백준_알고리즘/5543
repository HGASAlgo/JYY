import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int a = input.nextInt();
        int b = input.nextInt();
        int c = input.nextInt();
        int d = input.nextInt();
        int e = input.nextInt();
        int burger, drink;
        
        if (a<=b) {
            if (a<=c)
                burger = a;
            else 
                burger = c;
        }
        else {
            if (b<=c)
                burger = b;
            else 
                burger = c;
        }
        if (d<=e)
            drink = d;
        else
            drink = e;
        
        System.out.println(burger + drink - 50);
        
    }
}
